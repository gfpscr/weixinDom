}
.home_swiper swiper{
  width: 750rpx;
  height: 340rpx  ;
}
.home_swiper swiper image{
  width:100%;
}

.index_cate {
  width:100vW;
  display: flex;
  justify-items: center;
  align-content: center;
}
.index_cate>navigator{
  flex: 1;
}
.index_cate navigator image{
  width: 100%;
}
.index_floor .floor_group .floor_title{
  padding: 15rpx 0 0;
}
.index_floor .floor_group .floor_list{
  /* overflow: hidden; */
  height:calc(33.33vw * 368 / 232);
}
/* .index_floor .floor_group .floor_list::after{
  content: '';
  visibility: hidden;
  height: 0;
  display: block;
  clear: both;
} */
.index_floor .floor_group .floor_list navigator{
    width: 33.33%;
    float: left; 
}
.index_floor .floor_group .floor_list navigator:first-of-type{
  height:100%;
}
.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4){
    height:calc(33.33vw * 368 / 232 / 2 );
    border-left: 10rpx solid rgba(255, 255, 255, 1);
    box-sizing: border-box;
}
.index_floor .floor_group .floor_list navigator:nth-child(2),
.index_floor .floor_group .floor_list navigator:nth-child(3){
  border-bottom: 10rpx solid rgba(255,255,255,1);
}
.index_floor .floor_group .floor_list navigator image{
  width: 100%;
  height: 100%;
}