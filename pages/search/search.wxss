page{
  height: 100%;
}
.cates{height: 100%;}
.cates_container{
  height: calc(100vh - 96rpx);
  display: flex;
}
.cates_container .left_list{
  flex: 2;
}
.cates_container .left_list view{
  height: 70rpx;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center
}

.cates_container .right_list{
    flex:5;
}
.goods_group{

}
.goods_title{
  font-size: 30rpx;
  height: 70rpx;
  display: flex;
  justify-content:center;
  align-items:center;
}
.goods_title .title{}
.goods_title .decorate{
  display: inline-block;
  color: #ccc;
  padding: 0 10rpx;
}
.goods_lists{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.goods_lists navigator{
  /* flex: 1; */
  display: inline-block;
  width: 33.33%;
  text-align: center;
}
.goods_lists navigator image{
  width: 50%;
}
.goods_lists navigator view{
  text-align: center;
}
.active{
  color:var(--themeColor);
  border-left:5rpx solid currentColor;
}